{
  "generator-cg-angular": {
    "uirouter": true,
    "partialDirectory": "partial/",
    "directiveDirectory": "directive/",
    "filterDirectory": "filter/",
    "serviceDirectory": "service/",
    "partialTemplates": "../tools/cg-angular/partial",
    "directiveSimpleTemplates": "../tools/cg-angular/directive/simple",
    "directiveComplexTemplates": "../tools/cg-angular/directive/complex",
    "serviceTemplates": "../tools/cg-angular/service",
    "filterTemplates": "../tools/cg-angular/filter",
    "moduleTemplates": "../tools/cg-angular/module",
    "inject": {
      "js": {
        "file": "index.html",
        "marker": "<!-- Add New Component JS Above -->",
        "template": "<script src=\"<%= filename %>\"></script>"
      },
      "less": {
        "relativeToModule": true,
        "file": "<%= module %>.less",
        "marker": "/* Add Component LESS Above */",
        "template": "@import \"<%= filename %>\";"
      }
    },
    "modules": [
      {
        "name": "common",
        "file": "common/common.js"
      }
    ]
  }
}